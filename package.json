{
  "name": "humanity-cards",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.ts",
  "scripts": {
    "installall": "concurrently --kill-others \"yarn install\" \"yarn install-client\"",
    "install-client": "cd client && yarn install",
    "client": "cd client && yarn start",
    "server": "node -r ts-node/register --inspect=5001 --nolazy server/server.ts --runInBand",
    "server-watch": "nodemon",
    "clean": "if exist output rmdir /s /q output",
    "dev": "concurrently --kill-others \"yarn server-watch\" \"yarn client\"",
    "build-client": "cd client && yarn build",
    "build-server": "node server/scripts/build.js -serverenv=prod",
    "build-server-beta": "node server/scripts/build.js -serverenv=beta",
    "build-server-local": "node server/scripts/build.js -serverenv=local",
    "build": "yarn clean && yarn build-client && yarn build-server",
    "build-beta": "yarn clean && yarn build-client && yarn build-server-beta",
    "build-local": "yarn clean && yarn build-client && yarn build-server-local",
    "finalize": "node server/scripts/finalize.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.15.4",
