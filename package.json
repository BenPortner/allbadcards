    "cross-fetch": "^3.0.4",
    "eslint-plugin-import": "^2.20.1",
    "express": "4.17.1",
    "express-serve-static-core": "^0.1.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mongodb": "^3.3.2",
    "simple-oauth2": "^3.1.0",
    "ts-node": "^8.3.0",
    "typescript": "3.7.2",
    "node-fetch": "^2.6.0",
    "patreon": "^0.4.1",
    "redis": "^3.0.2",
    "winston": "^3.2.1",
    "ws": "^7.2.3",
    "shortid": "^2.2.15",
    "dotenv": "^8.2.0",
    "human-readable-ids": "^1.0.4",
    "js-levenshtein": "^1.1.6",
    "log-timestamp": "^0.3.0"
  },
